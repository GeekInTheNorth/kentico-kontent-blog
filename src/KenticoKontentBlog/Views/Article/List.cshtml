@model KenticoKontentBlog.Feature.ArticleList.ArticleListViewModel

@if (Model.Articles != null && Model.Articles.Any())
{
    <partial name="_ArticleList" model="Model.Articles" />
}
else
{
    <div class="container">
        <div class="row">
            <div class="col-lg-8 col-md-10 mx-auto">
                <p>No articles exist with for this category</p>
            </div>
        </div>
    </div>
}
